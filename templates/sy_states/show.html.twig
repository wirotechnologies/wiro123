<!DOCTYPE html>

<title>SyStates</title>

{% block body %}
    <h1>SyStates</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sy_state.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ sy_state.name }}</td>
            </tr>
            <tr>
                <th>CreatedDate</th>
                <td>{{ sy_state.createdDate ? sy_state.createdDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedDate</th>
                <td>{{ sy_state.updatedDate ? sy_state.updatedDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('sy_states_index') }}">back to list</a>

    <a href="{{ path('sy_states_edit', {'id': sy_state.id}) }}">edit</a>

    {{ include('sy_states/_delete_form.html.twig') }}
{% endblock %}
