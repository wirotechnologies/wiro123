<!DOCTYPE html>

<title>SyCities</title>

{% block body %}
    <h1>SyCities</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sy_city.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ sy_city.name }}</td>
            </tr>
            <tr>
                <th>CreatedDate</th>
                <td>{{ sy_city.createdDate ? sy_city.createdDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedDate</th>
                <td>{{ sy_city.updatedDate ? sy_city.updatedDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('sy_cities_index') }}">back to list</a>

    <a href="{{ path('sy_cities_edit', {'id': sy_city.id}) }}">edit</a>

    {{ include('sy_cities/_delete_form.html.twig') }}
{% endblock %}
