<!DOCTYPE html>

<title>SyNeighborhoods index</title>

{% block body %}
    <h1>SyNeighborhoods index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>CreatedDate</th>
                <th>UpdatedDate</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for sy_neighborhood in sy_neighborhoods %}
            <tr>
                <td>{{ sy_neighborhood.id }}</td>
                <td>{{ sy_neighborhood.name }}</td>
                <td>{{ sy_neighborhood.createdDate ? sy_neighborhood.createdDate|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ sy_neighborhood.updatedDate ? sy_neighborhood.updatedDate|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('sy_neighborhoods_show', {'id': sy_neighborhood.id}) }}">show</a>
                    <a href="{{ path('sy_neighborhoods_edit', {'id': sy_neighborhood.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('sy_neighborhoods_new') }}">Create new</a>
{% endblock %}
