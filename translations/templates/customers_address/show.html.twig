<!DOCTYPE html>

<title>CustomersAddress</title>

{% block body %}
    <h1>CustomersAddress</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ customers_address.id }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ customers_address.active ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>StartActive</th>
                <td>{{ customers_address.startActive ? customers_address.startActive|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>EndActive</th>
                <td>{{ customers_address.endActive ? customers_address.endActive|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CreatedDate</th>
                <td>{{ customers_address.createdDate ? customers_address.createdDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedDate</th>
                <td>{{ customers_address.updatedDate ? customers_address.updatedDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('customers_address_index') }}">back to list</a>

    <a href="{{ path('customers_address_edit', {'id': customers_address.id}) }}">edit</a>

    {{ include('customers_address/_delete_form.html.twig') }}
{% endblock %}
